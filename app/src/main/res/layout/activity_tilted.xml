<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="ctec.aboutme.TiltedActivity">

    <TextView android:text="@string/Tilt" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="26dp"
        android:id="@+id/Tilt"
        android:textSize="24dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ToAuto"
        android:background="@drawable/marksmanlarge"
        android:layout_marginTop="264dp"
        android:layout_below="@+id/Tilt"
        android:layout_toRightOf="@+id/Tilt"
        android:layout_toEndOf="@+id/Tilt"
        android:layout_marginLeft="41dp"
        android:layout_marginStart="41dp" />

</RelativeLayout>
