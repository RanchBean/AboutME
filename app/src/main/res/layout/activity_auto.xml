<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="ctec.aboutme.AutoActivity">

    <TextView android:text="@string/Auto" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Auto"
        android:textSize="24dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Equalizer"
        android:background="@drawable/ipad_31292_2d_black_and_white_equalizer"
        android:layout_marginTop="289dp"
        android:layout_below="@+id/Auto"
        android:layout_toRightOf="@+id/Auto"
        android:layout_toEndOf="@+id/Auto"
        android:layout_marginLeft="102dp"
        android:layout_marginStart="102dp" />

</RelativeLayout>
